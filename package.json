{
  "name": "message-board",
  "version": "1.0.0",
  "description": "Messaging App",
  "scripts": {
    "dev": "cd ./message-board-scripts && cmd.exe /c start-server-and-client.bat",
    "dev:client": "cd ./react-client && npm run dev",
    "dev:server": "cd ./server && npm run dev",
    "start:client": "cd ./react-client && npm run build && npm run preview",
    "start:server": "cd ./server && npm run tsc && npm start",
    "install-all": "bash ./message-board-scripts/install-all.sh"
  },
  "author": "Tamanji Che",
  "license": "ISC"
}
